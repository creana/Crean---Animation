//setup function
void setup() {
  size(600,400); // sets the canvas size
  background(50, 220, 50); 
  
  fill(0, 0, 0); //black fill
  stroke(255, 255, 255);
  strokeWeight(10);
  rect(300, 200, 100, 100); // draw rectangle
  
  fill(0, 0, 0);
  stroke(255, 255, 255);
  strokeWeight(10);
  triangle(300, 200, 350, 125, 400, 200);
  
} //end setup

